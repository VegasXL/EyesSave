<Application x:Class="EyesSave.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EyesSave"
             StartupUri="Presentation/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="ColorPrimary">#FFFFFF</SolidColorBrush>
            <SolidColorBrush x:Key="ColorSecond">#D3D3D3</SolidColorBrush>
            <SolidColorBrush x:Key="ColorAccent">#A163F5</SolidColorBrush>
        </ResourceDictionary>
    </Application.Resources>
</Application>
